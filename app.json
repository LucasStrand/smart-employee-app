{
  "expo": {
    "name": "smart-employee-app",
    "slug": "g",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logga_png.png",
    "scheme": "smart-employee-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logga_png.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "smart-employee-app",
              "host": "auth"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/logga_png.png"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://smartteknik.nu/"
        }
      ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logga_png.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
